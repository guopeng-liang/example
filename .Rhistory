setwd("/Users/gl/Library/CloudStorage/OneDrive-YaleUniversity/Yale/Research/Project/Tree mortality/NFI/NFI_ground_plot_data_approx_loc_v2.0")
library(readr)
library(dplyr)
library(ggplot2)
#tree mortality data
tree.remove <- read_delim("all_gp_ltp_tree_removed.csv", ",", col_names = TRUE)
tree.remove.mortality <- subset(tree.remove, tree.remove$reason!="C")
tree.remove.mortality.count <- tree.remove.mortality %>% count(nfi_plot)
dim(tree.remove.mortality.count)
tree.survey <- read_delim("all_gp_ltp_tree.csv", ",", col_names = TRUE)
tree.survey$yr <- substr(tree.survey$meas_date, 1, 4)
tree.survey$yr <- as.numeric(tree.survey$yr)
table(tree.survey$yr)
table(tree.survey$meas_num)
tree.survey1 <- subset(tree.survey, tree.survey$yr>=2000&tree.survey$yr<=2006)
tree.survey.live.count <- tree.survey1 %>% count(nfi_plot)
tree.survey.live.count1 <- subset(tree.survey.live.count, tree.survey.live.count$n>=20)
dim(tree.survey.live.count)
mortality <- left_join(tree.survey.live.count1, tree.remove.mortality.count, by="nfi_plot")
mortality$rate <- (mortality$n.y/mortality$n.x)*100
dim(mortality)
